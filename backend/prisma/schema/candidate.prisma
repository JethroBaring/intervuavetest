model Candidate {
  id         String      @id @default(cuid())
  firstName  String
  lastName   String
  email      String      @unique
  companyId  String
  company    Company     @relation(fields: [companyId], references: [id], onDelete: Cascade)
  interviews Interview[]
  createdAt  DateTime    @default(now())
  updatedAt  DateTime    @updatedAt
  deletedAt  DateTime?
}
